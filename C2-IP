int loop0
ip address 192.168.30.2/32
!
int eth1
description to PE2-E3
no switchport
ip address 192.168.32.2/30
no shut
!
mpls ip
!
ip prefix-list ldp-fec-prefix-list
  permit 192.168.100.0/24 ge 24
!
mpls ldp
  router-id interface lo0
  fec filter prefix-list ldp-fec-prefix-list
  no shut
!
ip route 0.0.0.0/0 192.168.32.1
